# ========================================
# Production Environment Configuration
# ========================================
# Copy this file to .env and fill in your values

# ========================================
# API Keys (REQUIRED)
# ========================================
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# ========================================
# Database Configuration
# ========================================
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/guidance_agent

# ========================================
# LLM Model Configuration
# ========================================
LITELLM_MODEL_ADVISOR=gpt-4-turbo-preview
LITELLM_MODEL_CUSTOMER=gpt-3.5-turbo
LITELLM_MODEL_EMBEDDINGS=text-embedding-3-small
EMBEDDING_DIMENSION=1536

# ========================================
# Phoenix Configuration
# ========================================
PHOENIX_COLLECTOR_ENDPOINT=http://phoenix:4317
PHOENIX_PROJECT_NAME=guidance-agent

# ========================================
# Application Settings
# ========================================
ENVIRONMENT=production
LOG_LEVEL=INFO

# ========================================
# Frontend Configuration
# ========================================
VITE_API_BASE_URL=http://localhost:8000
VITE_APP_TITLE=Pension Guidance Chat
VITE_ENVIRONMENT=production
